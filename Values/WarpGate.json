{
    //"Context" is an IWarpGate

    //Constant values
    "BaseRange": 1,         // LY
    "BaseLinkTime": 1,      // S
    "BasePayloadTime": 1,   // S/LY
    
    // Range this gate may link (Lightyears)
    "Range": "BaseRange",
    
    // Time it takes to establish a link to another gate (Seconds)
    "LinkTime": "BaseLinkTime + (BaseLinkTime * (Context.Distance / Range)) ^ 2",
    
    // Travel time along this link
    "PayloadTime": "Context.Distance * BasePayloadTime",
}