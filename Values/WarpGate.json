{
    //"Context" is an IWarpGate

    //Constant values
    "BaseRange": 1,         // LY
    "BaseLinkTime": 1,      // S
    "BasePayloadTime": 1,   // S/LY
    
    //Formulaic values
    "Range": "BaseRange",
    
    //Scale link time based on distance linking, distant links take exponentially longer
    "LinkTime": "BaseLinkTime + (BaseLinkTime * (Context.Distance / Range)) ^ 2",
    
    //Scale travel time by distance
    "PayloadTime": "Context.Distance * BasePayloadTime",
}